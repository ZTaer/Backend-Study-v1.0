										"Num": 4
									},
									"Properties": {
										"id": "20230608232107-k7n4mdv"
									},
									"Children": [
										{
											"ID": "20230608232107-ita5fbl",
											"Type": "NodeParagraph",
											"Properties": {
												"id": "20230608232107-ita5fbl",
												"updated": "20230608232337"
											},
											"Children": [
												{
													"Type": "NodeTextMark",
													"TextMarkType": "u",
													"TextMarkTextContent": "node.js 事件循环以及微任务队列关系Promise"
												}
											]
										},
										{
											"ID": "20230608232127-29fn9i2",
											"Type": "NodeList",
											"ListData": {
												"Typ": 1
											},
											"Properties": {
												"id": "20230608232127-29fn9i2"
											},
											"Children": [
												{
													"ID": "20230608232127-op3kpop",
													"Type": "NodeListItem",
													"ListData": {
														"Typ": 1,
														"Delimiter": 46,
														"Marker": "MS4=",
														"Num": 1
													},
													"Properties": {
														"id": "20230608232127-op3kpop"
													},
													"Children": [
														{
															"ID": "20230608232130-8by4xap",
															"Type": "NodeParagraph",
															"Properties": {
																"id": "20230608232130-8by4xap",
																"updated": "20230608232130"
															},
															"Children": [
																{
																	"Type": "NodeText",
																	"Data": "Node.js中的事件循环确实包括处理Promises，特别是处理Promise的回调函数。Promises通常在\"microtask queue\"（微任务队列）中处理，这与Node.js事件循环的\"macrotask\"（宏任务）阶段（例如timers、IO callbacks等）是分开的。"
																}
															]
														},
														{
															"ID": "20230608232130-tcbh9k0",
															"Type": "NodeParagraph",
															"Properties": {
																"id": "20230608232130-tcbh9k0",
																"updated": "20230608232130"
															},
															"Children": [
																{
																	"Type": "NodeText",
																	"Data": "以下是Promise在Node.js事件循环中的大致位置："
																}
															]
														},
														{
															"ID": "20230608232130-n1qumim",
															"Type": "NodeList",
															"ListData": {},
															"Properties": {
																"id": "20230608232130-n1qumim",
																"updated": "20230608232130"
															},
															"Children": [
																{
																	"ID": "20230608232130-2ew4sd8",
																	"Type": "NodeListItem",
																	"ListData": {
																		"BulletChar": 42,
																		"Marker": "Kg=="
																	},
																	"Properties": {
																		"id": "20230608232130-2ew4sd8",
																		"updated": "20230608232130"
																	},
																	"Children": [
																		{
																			"ID": "20230608232130-8swjcgk",
																			"Type": "NodeParagraph",
																			"Properties": {
																				"id": "20230608232130-8swjcgk",
																				"updated": "20230608232130"
																			},
																			"Children": [
																				{
																					"Type": "NodeText",
																					"Data": "当一个Promise被resolve或reject时，它的.then()或.catch()回调被添加到微任务队列中。"
																				}
																			]
																		}
																	]
																},
																{
																	"ID": "20230608232130-1en6qs5",
																	"Type": "NodeListItem",
																	"ListData": {
																		"BulletChar": 42,
																		"Marker": "Kg=="
																	},
																	"Properties": {
																		"id": "20230608232130-1en6qs5",
																		"updated": "20230608232130"
																	},
																	"Children": [
																		{
																			"ID": "20230608232130-d2ellva",
																			"Type": "NodeParagraph",
																			"Properties": {
																				"id": "20230608232130-d2ellva",
																				"updated": "20230608232130"
																			},
																			"Children": [
																				{
																					"Type": "NodeText",
																					"Data": "在每个事件循环阶段的结束以及某些API的调用后，Node.js会检查并清空微任务队列，也就是说它会运行所有待处理的Promise回调。"
																				}
																			]
																		}
																	]
																},
																{
																	"ID": "20230608232130-wr328ue",
																	"Type": "NodeListItem",
																	"ListData": {
																		"BulletChar": 42,
																		"Marker": "Kg=="
																	},
																	"Properties": {
																		"id": "20230608232130-wr328ue",
																		"updated": "20230608232130"
																	},
																	"Children": [
																		{
																			"ID": "20230608232130-p3ada6t",
																			"Type": "NodeParagraph",
																			"Properties": {
																				"id": "20230608232130-p3ada6t",
																				"updated": "20230608232130"
																			},
																			"Children": [
																				{
																					"Type": "NodeText",
																					"Data": "然后事件循环继续下一个阶段。"
																				}
																			]
																		}
																	]
																}
															]
														},
														{
															"ID": "20230608232130-1nj0sez",
															"Type": "NodeParagraph",
															"Properties": {
																"id": "20230608232130-1nj0sez",
																"updated": "20230608232130"
															},
															"Children": [
																{
																	"Type": "NodeText",
																	"Data": "因此，你可以说Promise（实际上是它们的回调）有高于IO callbacks（包括网络、文件系统等的回调）但低于其他阶段如timer callbacks（setTimeout和setInterval的回调）的优先级。换句话说，Promise的回调总是在Node.js进入下一个事件循环阶段前被执行。"
																}
															]
														},
														{
															"ID": "20230608232130-af0a062",
															"Type": "NodeParagraph",
															"Properties": {
																"id": "20230608232130-af0a062",
																"updated": "20230608232215"
															},
															"Children": [
																{
																	"Type": "NodeText",
																	"Data": "这里的关键是，尽管Promise回调在微任务队列中，但是它们并不属于事件循环的任何一个特定阶段。相反，"
																},
																{
																	"Type": "NodeTextMark",
																	"TextMarkType": "u",
																	"TextMarkTextContent": "微任务队列在每个阶段完成后都会被处理。"
																}
															]
														},
														{
															"ID": "20230608232130-mpr97g5",
															"Type": "NodeParagraph",
															"Properties": {
																"id": "20230608232130-mpr97g5",
																"updated": "20230608232130"
															},
															"Children": [
																{
																	"Type": "NodeText",
																	"Data": "要注意的是，async/await也是基于Promise的，当你在async函数中使用await时，它实际上是在微任务队列中添加了一个Promise的回调。"
																}
															]
														},
														{
															"ID": "20230608232130-lc0l7ul",
															"Type": "NodeParagraph",
															"Properties": {
																"id": "20230608232130-lc0l7ul",
																"updated": "20230608232130"
															},
															"Children": [
																{
																	"Type": "NodeText",
																	"Data": "这是一个简化的解释，真实的Node.js事件循环和微任务处理过程可能会更复杂，但这个模型可以帮助你理解Promise在事件循环中的处理方式。"
																}
															]
														}
													]
												}
											]
										}
									]
								},
								{
