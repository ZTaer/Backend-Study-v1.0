																"id": "20230608232035-v7rkoon",
																"updated": "20230608232035"
															},
															"Children": [
																{
																	"ID": "20230608232035-sxiovzj",
																	"Type": "NodeListItem",
																	"ListData": {
																		"BulletChar": 42,
																		"Marker": "Kg=="
																	},
																	"Properties": {
																		"id": "20230608232035-sxiovzj",
																		"updated": "20230608232035"
																	},
																	"Children": [
