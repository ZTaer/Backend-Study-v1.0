																			"Children": [
																				{
																					"Type": "NodeText",
																					"Data": "setImmediate()总是优先于setTimeout()。"
																				}
																			]
																		}
																	]
																},
																{
																	"ID": "20230608232035-uttrqen",
																	"Type": "NodeListItem",
																	"ListData": {
																		"BulletChar": 42,
																		"Marker": "Kg=="
																	},
																	"Properties": {
																		"id": "20230608232035-uttrqen",
																		"updated": "20230608232035"
